        "glob": "^11.0.0",
        "hono": "^4.6.11",
        "import-sort": "^6.0.0",
        "vitest": "^2.1.5"
    },
    "engines": {
        "node": ">=18"
    },
    "main": "./dist/index.js",
    "types": "./dist/types.d.ts",
    "module": "./dist/index.js",
    "files": [
        "./dist/types.d.ts",
        "./dist/utils.js",
        "./dist/discord/**/*.js",
        "./package.json"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js",
